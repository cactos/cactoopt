package eu.cactosfp7.cactoopt.optimisationservice.resourcecontrol.json;
public class RequestVirtualMachine {
	private String id;
	private int maxCapacity;
	private int targetResponseTime;
	private int measuredReponseTime;

	public RequestVirtualMachine(String id, int maxCapacity, int targetResponseTime, int measuredReponseTime) {
		super();
		this.id = id;
		this.maxCapacity = maxCapacity;
		this.targetResponseTime = targetResponseTime;
		this.measuredReponseTime = measuredReponseTime;
	}
}