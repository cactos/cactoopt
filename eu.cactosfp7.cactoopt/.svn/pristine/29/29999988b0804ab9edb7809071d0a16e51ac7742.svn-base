package eu.cactosfp7.cactoopt.models;
import java.util.Comparator;


public class VirtualMachineMemoryComparator implements Comparator<VirtualMachine> {
    @Override
    public int compare(VirtualMachine o1, VirtualMachine o2) {
    	Double o1Memory = new Double(o1.getMemory());
    	Double o2Memory = new Double(o2.getMemory());
    	
        return o1Memory.compareTo(o2Memory);
    }
}
